{
    "manifest_version": 3,
    "name": "Bookmark Duplicate Remover",
    "version": "1.0",
    "description": "Find and delete duplicate bookmarks.",
    "permissions": ["bookmarks"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  