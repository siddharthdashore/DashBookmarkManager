{
    "manifest_version": 3,
    "name": "Dash Bookmark Manager",
    "version": "0.5.0",
    "description": "Search and delete bookmarks, duplicates, and empty folders (individually or in bulk) for simple, efficient bookmarks management.",
    "permissions": ["bookmarks"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "dash-icon16x16.png",
        "32": "dash-icon32x32.png",
        "48": "dash-icon48x48.png",
        "128": "dash-icon128x128.png"
    }
  }
  